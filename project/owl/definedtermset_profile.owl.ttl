@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix definedtermset_profile: <https://w3id.org/stroemphi/DefinedTermSet_profile/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema1: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

definedtermset_profile:CreativeWork a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "CreativeWork" ;
    rdfs:subClassOf definedtermset_profile:Thing ;
    skos:exactMatch schema1:CreativeWork ;
    skos:inScheme <https://w3id.org/stroemphi/DefinedTermSet_profile> .

definedtermset_profile:Intangible a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Intangible" ;
    rdfs:subClassOf definedtermset_profile:Thing ;
    skos:definition "A utility class that serves as the umbrella for a number of 'intangible' things such as quantities, structured values, etc." ;
    skos:exactMatch schema1:Intangible ;
    skos:inScheme <https://w3id.org/stroemphi/DefinedTermSet_profile> .

definedtermset_profile:DefinedTerm a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "DefinedTerm" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty definedtermset_profile:inDefinedTermSet ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty definedtermset_profile:termCode ],
        [ a owl:Restriction ;
            owl:allValuesFrom definedtermset_profile:DefinedTermSet ;
            owl:onProperty definedtermset_profile:inDefinedTermSet ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty definedtermset_profile:termCode ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty definedtermset_profile:termCode ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty definedtermset_profile:inDefinedTermSet ],
        definedtermset_profile:Intangible ;
    skos:definition "A word, name, acronym, phrase, etc. with a formal definition. Often used in the context of category or subject classification, glossaries or dictionaries, product or creative work types, etc. Use the name property for the term being defined, use termCode if the term has an alpha-numeric code allocated, use description to provide the definition of the term." ;
    skos:exactMatch schema1:DefinedTerm ;
    skos:inScheme <https://w3id.org/stroemphi/DefinedTermSet_profile> .

definedtermset_profile:DefinedTermSet a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "DefinedTermSet" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty definedtermset_profile:hasDefinedTerm ],
        [ a owl:Restriction ;
            owl:allValuesFrom definedtermset_profile:DefinedTerm ;
            owl:onProperty definedtermset_profile:hasDefinedTerm ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty definedtermset_profile:hasDefinedTerm ],
        definedtermset_profile:CreativeWork ;
    skos:definition "A set of defined terms for example a set of categories or a classification scheme, a glossary, dictionary or enumeration." ;
    skos:exactMatch schema1:DefinedTermSet ;
    skos:inScheme <https://w3id.org/stroemphi/DefinedTermSet_profile> .

definedtermset_profile:Thing a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Thing" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty definedtermset_profile:description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty definedtermset_profile:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty definedtermset_profile:alternateName ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty definedtermset_profile:name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty definedtermset_profile:sameAs ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty definedtermset_profile:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty definedtermset_profile:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Uriorcurie ;
            owl:onProperty definedtermset_profile:url ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty definedtermset_profile:sameAs ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty definedtermset_profile:sameAs ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty definedtermset_profile:alternateName ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Uriorcurie ;
            owl:onProperty definedtermset_profile:id ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty definedtermset_profile:id ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty definedtermset_profile:url ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty definedtermset_profile:url ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty definedtermset_profile:alternateName ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty definedtermset_profile:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty definedtermset_profile:id ] ;
    skos:definition "The most generic type of item." ;
    skos:exactMatch schema1:Thing ;
    skos:inScheme <https://w3id.org/stroemphi/DefinedTermSet_profile> .

definedtermset_profile:alternateName a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "alternateName" ;
    skos:inScheme <https://w3id.org/stroemphi/DefinedTermSet_profile> .

definedtermset_profile:description a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "description" ;
    skos:definition "A human-readable description for a thing" ;
    skos:inScheme <https://w3id.org/stroemphi/DefinedTermSet_profile> .

definedtermset_profile:hasDefinedTerm a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "hasDefinedTerm" ;
    rdfs:range definedtermset_profile:DefinedTerm ;
    skos:definition "A Defined Term contained in this term set." ;
    skos:inScheme <https://w3id.org/stroemphi/DefinedTermSet_profile> .

definedtermset_profile:id a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range linkml:Uriorcurie ;
    skos:definition "A unique identifier for a thing" ;
    skos:inScheme <https://w3id.org/stroemphi/DefinedTermSet_profile> .

definedtermset_profile:inDefinedTermSet a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inDefinedTermSet" ;
    rdfs:range definedtermset_profile:DefinedTermSet ;
    skos:definition "A DefinedTermSet that contains this term." ;
    skos:inScheme <https://w3id.org/stroemphi/DefinedTermSet_profile> .

definedtermset_profile:name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name" ;
    skos:definition "A human-readable name for a thing" ;
    skos:inScheme <https://w3id.org/stroemphi/DefinedTermSet_profile> .

definedtermset_profile:sameAs a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "sameAs" ;
    skos:definition "URL of a reference Web page that unambiguously indicates the item's identity. E.g. the URL of the item's Wikipedia page, Wikidata entry, or official website." ;
    skos:inScheme <https://w3id.org/stroemphi/DefinedTermSet_profile> .

definedtermset_profile:termCode a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "termCode" ;
    skos:definition "A code that identifies this DefinedTerm within a DefinedTermSet." ;
    skos:inScheme <https://w3id.org/stroemphi/DefinedTermSet_profile> .

definedtermset_profile:url a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "url" ;
    rdfs:range linkml:Uriorcurie ;
    skos:definition "URL of the item." ;
    skos:inScheme <https://w3id.org/stroemphi/DefinedTermSet_profile> .

<https://w3id.org/stroemphi/DefinedTermSet_profile> a owl:Ontology ;
    rdfs:label "DefinedTermSet_profile" ;
    dcterms:license "MIT" ;
    dcterms:title "DefinedTermSet_profile" ;
    rdfs:seeAlso <https://stroemphi.github.io/DefinedTermSet_profile> ;
    skos:definition "A LinkML schema to define a profile for the DefinedTermSet class from Schema.org." .

